<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="Background" Color="#1E1E1E" />
    <SolidColorBrush x:Key="border" Color="#3C3C3C" />

    <SolidColorBrush x:Key="Foreground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ForegroundSelected" Color="#f9474b"/>
    <SolidColorBrush x:Key="ForegroundUnselected" Color="#6c6d6f"/>
    
    
    <FontFamily x:Key="Inter">pack://application:,,,/fonts/#Inter</FontFamily>
    <FontFamily x:Key="Nunito">pack://application:,,,/fonts/#Nunito</FontFamily>
    <FontFamily x:Key="Segoeicons">pack://application:,,,/fonts/#Segoe Fluent Icons</FontFamily>
    <FontFamily x:Key="tgxicons">pack://application:,,,/fonts/tgx-fluent-icons.ttf#tgx-fluent-icons</FontFamily>

</ResourceDictionary> 